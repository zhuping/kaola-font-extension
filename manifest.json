{
  "manifest_version": 2,
  "name": "font字体上传",
  "description": "iconfont字体上传至nos",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "*://at.alicdn.com/*",
    "*://nos.kaolafed.com/upload/*"
  ],
  "icons": {
    "16": "icon/icon_16.png",
    "32": "icon/icon_32.png",
    "128": "icon/icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon/icon_32.png",
    "default_title": "font字体上传",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://iconfont.cn/*"],
      "js": ["js/getCode.js"],
      "run_at": "document_end"
    }
  ]
}